{
  "name": "fityme",
  "version": "1.0.0",
  "description": "A Stylus library to generate skeleton screens, through svg placeholder as data URI background in order to help you improve the perceived loading time of your apps.",
  "main": "index.styl",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "stylus --watch test/*.styl -o test",
    "build": "stylus bundle.styl -o dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manobi/fityme.git"
  },
  "keywords": [
    "stylus",
    "skeleton",
    "screen",
    "placeholder",
    "fityme",
    "ui",
    "loader"
  ],
  "author": "Gabriel de Oliveira Barbosa (Manobi)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manobi/fityme/issues"
  },
  "homepage": "https://github.com/manobi/fityme#readme"
}
